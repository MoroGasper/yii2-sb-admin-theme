{
    "name": "p2made/yii2-sb-admin-theme",
    "type": "yii2-extension",
    "version": "1.0.0",
    "license": "MIT",

    "description": "SB Admin from Start Bootstrap, http://startbootstrap.com/, as a backend UI for Yii 2 Framework.",
    "keywords": [
        "yii", "yii2", "PHP",
        "asset", "bundle",
        "Start-Bootstrap", "sb-admin-2",
        "backend", "admin", "ui", "user-interface", "theme"
    ],
    "authors": [
        {
            "name": "Pedro Plwoman",
            "email": "pedro@p2made.net.au",
            "homepage": "http://p2made.net.au/"
        }
    ],

    "minimum-stability": "dev",

    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*",
        "bower-asset/moment" : "*",
        "bower-asset/moment-timezone" : "*",
        "datatables/datatables": "1.*",
        "onokumus/metismenu": "2.*",
        "philippfrenzel/yii2fullcalendar": "*",
        "rmrevin/yii2-fontawesome": "2.9.*",
        "p2made/yii2-asset-collection": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "p2made\\yii2theme\\sbAdmin\\": ""
        }
    }
}
